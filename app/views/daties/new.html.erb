<div id="T">
  <%= form_tag url_for(action:'create'), method:'post' do %>
    <div class="form-group row">
      <label for="example-datetime-local-input" class="col-2 col-form-label">Début du propagande</label>
      <div class="col-10">
        <input class="form-control" type="datetime-local" id="example-datetime-local-input" min= <%=Time.now%>>
      </div>
    </div>

    <div class="form-group row">
      <label for="example-datetime-local-input" class="col-2 col-form-label">Fin du propagande</label>
      <div class="col-10">
        <input class="form-control" type="datetime-local"  id="example-datetime-local-input" min= <%=Time.now%>>
      </div>
    </div>

    <div class="form-group row">
      <label for="example-datetime-local-input" class="col-2 col-form-label">Début du vote</label>
      <div class="col-10">
        <input class="form-control" type="datetime-local" id="example-datetime-local-input" min= <%=Time.now%>>
      </div>
    </div>

    <div class="form-group row">
      <label for="example-datetime-local-input" class="col-2 col-form-label">Fin du vote</label>
      <div class="col-10">
        <input class="form-control" type="datetime-local"  id="example-datetime-local-input" min= <%=Time.now%>>
      </div>
    </div>

      <input type="submit" value="Créer" class="btn btn-info size">
  <%end%>
</div>
	


