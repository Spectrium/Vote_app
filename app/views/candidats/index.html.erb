<!--	card bootstrap	-->
<h1 class="top">Liste de tous les candidats : </h1>
  <div class="card mb-3" style="max-width: 540px;">
  <% @candidat.each do |candidat| %>   

    <div class="row no-gutters">  
        <div class="col-md-4">
       <%= image_tag  "data:image/jpeg;base64," %>
        </div>

        <div class="col-md-8">
          <div class="card-body">
                <h5 class="card-title"><%= candidat.full_name %></h5>
                <p class="card-text"><%= candidat.description %></p>
                <p class="card-text"><small class="text-muted">Mouvance : <%= candidat.parti %></small></p>
            <%= link_to "Edit",edit_candidat_path(candidat.id), class:"btn btn-outline-success" %> 
            <%= link_to "Supprimer", candidat_path(candidat.id), method:"delete", data:{confirm:"Supprimé?"},class:"btn btn-outline-danger" %>
          </div>
     </div>        
  <% end %>
